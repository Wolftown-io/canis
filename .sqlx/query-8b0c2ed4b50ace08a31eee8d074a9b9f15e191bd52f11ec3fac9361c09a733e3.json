  "query": "\n        SELECT\n            message_id,\n            emoji,\n            COUNT(*) as \"count!\",\n            BOOL_OR(user_id = $1) as \"me!\"\n        FROM message_reactions\n        WHERE message_id = ANY($2)\n        GROUP BY message_id, emoji\n        ORDER BY MIN(created_at)\n        ",
